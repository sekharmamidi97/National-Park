@model List<Capstone.Web.Models.SurveyModel>
@{
    ViewBag.Title = "DisplaySurvey";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section>
    <h2>Survey Results</h2>

    @foreach (var survey in Model)
    {
        <table>
            <tr>
                <td>
                    @survey.parkName[survey.ParkCode]
                </td>
                <td>
                    @survey.NumberOfVotes
                </td>
            </tr>
        </table>

    }
</section>